programa {
  funcao inicio() {
   logico booleano1, booleano2

escreva("Digite verdadeiro ou falso (como 'verdadeiro' ou 'falso'): ")
leia(booleano1)
escreva("Digite verdadeiro ou falso (como 'verdadeiro' ou 'falso'): ")
leia(booleano2)

escreva("Ambos são verdadeiros? \n")

se ("booleano1 = verdadeiro e booleano2 = verdadeiro") 
    escreva("Resultado: Ambos são verdadeiros.\n")
senao
    escreva("Resultado: Pelo menos um é falso.\n") 
  }
}
