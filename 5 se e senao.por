programa {
  funcao inicio() {
  inteiro num1, num2

escreva("Digite dois números:")
leia(num1, num2)

se ("num1 = num2")
    escreva("Os números são iguais.", "\n")
senao
    escreva("Os números são diferentes.", "\n")

escreva("O primeiro número é maior ou igual a 20? ", num1 >= 20, "\n")  
  }
}
