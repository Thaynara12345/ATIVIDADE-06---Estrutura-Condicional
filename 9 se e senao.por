programa {
  funcao inicio() {
    inteiro numero

escreva("Digite um número: ")
leia(numero)

se (numero % 3 != 0) 
    escreva("O número é múltiplo de 3.\n")
senao
    escreva("O número não é múltiplo de 3.\n")

se (numero % 5 != 0)
escreva("O número não é múltiplo de 5.")
senao
    escreva("O número é múltiplo de 5.")

   
  }
}
